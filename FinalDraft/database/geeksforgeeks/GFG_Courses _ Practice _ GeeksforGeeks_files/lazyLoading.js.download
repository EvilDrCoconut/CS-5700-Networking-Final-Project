var lazyImage=[];var loaded=[];var count=0;window.addEventListener("scroll",function(ev){lazyload()});window.onload=function(){var lazyImageNode=document.getElementsByClassName("lazy");lazyImage=Array.prototype.slice.call(lazyImageNode);var lazyImageNode1=document.getElementsByClassName("banner-full");var lazyImageNode2=document.getElementsByClassName("banner-mobile");var lazyImage1=Array.prototype.slice.call(lazyImageNode1);var lazyImage2=Array.prototype.slice.call(lazyImageNode2);for(var i=0;i<lazyImage1.length;i++){lazyImage.push(lazyImage1[i])}for(var i=0;i<lazyImage2.length;i++){lazyImage.push(lazyImage2[i])}for(var i=0;i<lazyImage.length;i++){loaded.push(i)}lazyload()};function lazyload(){if(loaded.length==0){return}var temp=[];while(loaded.length>0){var ind=loaded.pop();if(elementInViewPort(lazyImage[ind])){if(lazyImage[ind].className=="banner-full"){lazyImage[ind].setAttribute("display","block")}lazyImage[ind].setAttribute("src",lazyImage[ind].getAttribute("data_src"))}else{temp.push(ind)}}while(temp.length>0){loaded.push(temp.pop())}}function elementInViewPort(el){var rect=el.getBoundingClientRect();return rect.top>=0&&rect.top+100<=(window.innerHeight||document.documentElement.clientHeight)&&rect.left<=(window.innerWidth||document.documentElement.clientWidth)}